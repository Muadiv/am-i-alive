[Unit]
Description=Am I Alive Vote Checker
After=network.target docker.service

[Service]
Type=oneshot
User=muadiv
WorkingDirectory=/home/muadiv/Code/am-i-alive
ExecStart=/usr/bin/python3 /home/muadiv/Code/am-i-alive/scripts/vote_checker.py
StandardOutput=journal
StandardError=journal

[Install]
WantedBy=multi-user.target
